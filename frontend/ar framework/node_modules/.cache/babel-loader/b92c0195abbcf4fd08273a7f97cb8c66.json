num: function (newValue, oldValue) {\r\n            this.targetdata = [];\r\n            console.log(\"x\");\r\n\r\n            for (let i = 0; i < newValue; i++) {\r\n                this.targetdata.push({\r\n                    targetname: \"\",\r\n                    scriptname: \"\",\r\n                    type: \"\"\r\n                });\r\n            }\r\n        }\r\n    }\r\n    /* eslint-enable */\r\n    ,\r\n    methods: {\r\n        writeFile: function (event) {\r\n            // `event` is the native DOM event\r\n            if (event) {\r\n                this.targetjson = {projectname:this.projectname, target:this.targetdata}\r\n                var FileSaver = require('file-saver');\r\n                var blob = new Blob([JSON.stringify(this.targetjson)], {type: \"text/plain;charset=utf-8\"});\r\n                FileSaver.saveAs(blob, \"Target.JSON\");\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n\r\n<style scoped>\r\nh3 {\r\n    margin: 40px 0 0;\r\n}\r\n\r\nul {\r\n    list-style-type: none;\r\n    padding: 0;\r\n}\r\n\r\nli {\r\n    display: inline-block;\r\n    margin: 0 10px;\r\n}\r\n\r\na {\r\n    color: #42b983;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}